        <ws:partial template="Controls/grid:EmptyTemplate" topSpacing="xl" bottomSpacing="l">
          <ws:contentTemplate>Не найдено</ws:contentTemplate>
        </ws:partial>
      </ws:emptyTemplate>
    </Controls.explorer:View>
