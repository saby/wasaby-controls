<Controls.Container.Async
    templateName="{{ _options.pageTemplate }}">
    <ws:partial
        on:preloadItemsByKeys="_preloadItemsByKeysHandler()"
        template="{{ content.resolvedTemplate }}"
        prefetchResult="{{ _prefetchData }}"
        pageId="{{ _options.pageId }}"
        isPrefetchDataMode="{{true}}"
        scope="{{ _options.pageTemplateOptions }}"
        />
</Controls.Container.Async>
