<ws:if data="{{_options.nodeProperty}}">
   <Controls._filterPopup.SimplePanel._HierarchyList
           scope="{{_options}}"
      />
</ws:if>
<ws:else data="{{_options.editorTemplateName === 'Controls/filterPanelEditors:DateMenu'}}">
   <Controls.Container.Async templateName="Controls/filterPanelEditors:DateMenu"
                             >
      <ws:partial template="{{ content.resolvedTemplate }}"
                  viewMode="frequent"
                  scope="{{_options}}"
                  />
   </Controls.Container.Async>
</ws:else>
<ws:else>
   <Controls._filterPopup.SimplePanel._List
      scope="{{_options}}"
   />
</ws:else>
