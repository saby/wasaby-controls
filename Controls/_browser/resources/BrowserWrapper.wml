<ws:if data="{{_options.dataStoreId}}">
    <Controls.baseList:DataContainer scope="{{_options}}" storeId="{{_options.dataStoreId}}"/>
</ws:if>
<ws:else>
    <Controls._browser.ContextResolver>
        <Controls._browser.Browser
            name="browser"
            scope="{{_options}}"/>
    </Controls._browser.ContextResolver>
</ws:else>