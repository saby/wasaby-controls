<div attr:class="controls-MonthSlider-Slider controls_calendar_theme-{{_options.theme}}" attr:animationState="{{_animationState}}">
   <ws:for data="index, item in _items">
      <div attr:class="controls-MonthSlider-Slider__item {{item.transitionClasses}}" attr:name="{{'container' + index}}">
         <ws:partial
            template="{{_options.content}}"
            data="{{item.data}}"
         />
      </div>
   </ws:for>
</div>