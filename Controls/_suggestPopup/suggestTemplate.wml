<ws:partial template="wml!Controls/_suggestPopup/WrappedList">
  <Controls.list:View keyProperty="{{_options.keyProperty}}"
                      sourceController="{{_options.sourceController}}">
    <ws:itemTemplate>
      <ws:partial template="{{_options.suggestItemTemplate || 'Controls/list:ItemTemplate'}}" displayProperty="{{displayProperty}}"/>
    </ws:itemTemplate>
  </Controls.list:View>
</ws:partial>
