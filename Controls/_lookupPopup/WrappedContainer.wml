<Controls._lookupPopup.__ControllerContextConsumer scope="{{ _options }}">
    <ws:controllerOptionsInnerComponent>
        <Controls.context:ContextOptionsConsumer scope="{{ controllerOptionsInnerComponent }}">
            <ws:contextOptionsInnerComponent>
                <Controls._lookupPopup.Container scope="{{ contextOptionsInnerComponent }}" _dataOptionsValue="{{contextOptionsInnerComponent._dataOptionsValue}}" />
            </ws:contextOptionsInnerComponent>
        </Controls.context:ContextOptionsConsumer>
    </ws:controllerOptionsInnerComponent>
</Controls._lookupPopup.__ControllerContextConsumer>
