<ws:if data="{{column.config.result }}">
   <Controls.baseDecorator:Money value="{{column.result}}"
                             fontColorStyle="{{column.displayProperty === 'sum' ? 'results' : 'unaccented'}}"
                             fontSize="l"
                             fontWeight="bold"
                             useGrouping="{{true}}"/>
</ws:if>
<ws:else>
   <invisible-node />
</ws:else>
