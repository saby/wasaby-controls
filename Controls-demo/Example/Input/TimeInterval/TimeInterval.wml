<div class="example-TimeInterval">
   <Controls-demo.Example.resource.BaseDemoInput title="Time interval input field" blocker="{{true}}">
      <ws:contents>
         <ws:Array>
            <ws:Object autoWidth="{{true}}">
               <ws:template>
                  <Controls.input:TimeInterval bind:value="_default1Value" mask="HH:MM"/>
               </ws:template>
            </ws:Object>
            <ws:Object autoWidth="{{true}}" spacing="normal">
               <ws:template>
                  <Controls.input:TimeInterval bind:value="_default2Value" mask="HH:MM:SS" attr:class="Controls-Demo_TimeInterval__HHMMSS"/>
               </ws:template>
            </ws:Object>
            <ws:Object autoWidth="{{true}}" spacing="normal">
               <ws:template>
                  <Controls.input:TimeInterval bind:value="_default3Value" mask="HHH:MM"/>
               </ws:template>
            </ws:Object>
            <ws:Object autoWidth="{{true}}" spacing="normal">
               <ws:template>
                  <Controls.input:TimeInterval bind:value="_default4Value" mask="HHH:MM:SS"/>
               </ws:template>
            </ws:Object>
         </ws:Array>
      </ws:contents>
   </Controls-demo.Example.resource.BaseDemoInput>
</div>
