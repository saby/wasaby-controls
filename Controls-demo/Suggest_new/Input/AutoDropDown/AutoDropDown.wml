<div class="controlsDemo__wrapper controlsDemo__flexRow">
    <div class="controlsDemo__wrapper__horizontal controlsDemo__cell">
         <div class="controls-text-label">Открытие автодополнения при фокусе</div>
         <Controls.SuggestInput
                 attr:class="controlsDemo__input demo-SuggestNew__auto"
                 bind:value="_suggestDownValue"
                 displayProperty="title"
                 searchParam="title"
                 autoDropDown="{{true}}"
                 navigation="{{_navigation}}"
                 source="{{_source}}">
             <ws:suggestTemplate templateName="wml!Controls-demo/Suggest_new/Input/AutoDropDown/resources/SuggestTemplate">
                 <ws:templateOptions demoClass="demo-SuggestInputList"/>
             </ws:suggestTemplate>
         </Controls.SuggestInput>
    </div>
</div>
