<div class="controlsDemo__wrapper controlsDemo__flexRow">
    <div class="controlsDemo__wrapper__horizontal controlsDemo__cell">
         <div class="controls-text-label">Отображение в поле ввода имени руководителя отдела</div>
         <Controls.SuggestInput
                 attr:class="controlsDemo__input demo-SuggestNew__displayProperty"
                 bind:value="_suggestDownValue"
                 displayProperty="owner"
                 searchParam="title"
                 placeholder="Введите название отдела"
                 navigation="{{_navigation}}"
                 source="{{_source}}">
             <ws:suggestTemplate templateName="wml!Controls-demo/Suggest_new/Input/DisplayProperty/resources/SuggestTemplate">
                 <ws:templateOptions demoClass="demo-SuggestInputList"/>
             </ws:suggestTemplate>
         </Controls.SuggestInput>
    </div>
</div>
