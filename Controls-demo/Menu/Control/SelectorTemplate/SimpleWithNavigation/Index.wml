<div class="controlsDemo__flexRow">
   <div class="controlsDemo__wrapper">
      <div class="controls-text-label">C navigation</div>
      <Controls.menu:Control
              source="{{_source}}"
              keyProperty="key"
              displayProperty="title"
              selectedKeys="{{_selectedKeys}}"
              selectorDialogResult="{{_resultSingleSelect}}"
              navigation="{{_navigation}}">
         <ws:selectorTemplate templateName="Controls-demo/Menu/Control/SelectorTemplate/StackTemplate">
            <ws:templateOptions multiSelect="{{false}}" items="{{_items}}"/>
         </ws:selectorTemplate>
      </Controls.menu:Control>
   </div>
</div>