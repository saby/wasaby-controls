<div class="controlsDemo__wrapper">
    <Controls.grid:View
            attr:style="max-width: 274px;"
            showEditArrow="{{true}}"
            storeId="ShowEditArrow"
            columns="{{_columns}}"
            on:editArrowClick="_editArrowClick()"/>
    <div if="{{_clickedKey}}" attr:data-qa="indicator-clicked-on">Кликнули по стрелке на записи с ключом {{_clickedKey}}</div>
</div>
