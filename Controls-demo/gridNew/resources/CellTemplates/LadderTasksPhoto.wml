<ws:partial template="Controls/grid:ColumnTemplate"
            attr:class="js-controls-ListView__notEditable"
            scope="{{ _options }}">
    <ws:contentTemplate>
        <ws:partial template="{{contentTemplate.ladderWrapper}}"
                    ladderProperty="photo">
            <img src="{{item.contents.photo}}" style="width: {{item.contents.width || 'auto'}}; height: {{item.contents.height || 'auto'}};"/>
        </ws:partial>
    </ws:contentTemplate>
</ws:partial>
