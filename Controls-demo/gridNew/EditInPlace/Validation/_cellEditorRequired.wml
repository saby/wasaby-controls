<ws:partial template="Controls/grid:ColumnTemplate" scope="{{_options}}">
<ws:contentTemplate>
    <ws:partial template="Controls/list:EditingTemplate"
                value="{{ item.contents.required }}"
                scope="{{ _options }}"
                enabled="{{true}}">
        <ws:editorTemplate>
            <Controls.validate:InputContainer name="InputValidate">
                <ws:validators>
                    <ws:Function value="{{ item.contents.required }}">Controls/validate:isRequired</ws:Function>
                </ws:validators>
                <ws:content>
                    <Controls.input:Text contrastBackground="{{true}}" bind:value="item.contents.required"
                                         selectOnClick="{{ false }}" />
                </ws:content>
            </Controls.validate:InputContainer>
        </ws:editorTemplate>
    </ws:partial>
</ws:contentTemplate>
</ws:partial>
