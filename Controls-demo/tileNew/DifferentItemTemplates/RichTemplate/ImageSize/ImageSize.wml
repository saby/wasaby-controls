<div class="controlsDemo__wrapper">
   <Controls.scroll:Container class="controlsDemo__height500 controlsDemo__maxWidth1000">
      <Controls.tile:View
              storeId="listData"
              tileMode="static"
              imageProperty="image"
              tileWidth="240">
         <ws:itemTemplate>
            <ws:partial
                    scope="{{ itemTemplate }}"
                    template="Controls/tile:RichTemplate"
                    imageSize="{{ itemTemplate.item.contents.imageSize }}"/>
         </ws:itemTemplate>
      </Controls.tile:View>
   </Controls.scroll:Container>
</div>
