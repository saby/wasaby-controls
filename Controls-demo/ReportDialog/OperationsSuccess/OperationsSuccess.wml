<div class="controlsDemo__wrapper controlsDemo__flexRow">
  <div class="controlsDemo__wrapper__horizontal controlsDemo__cell">
     <div class="controls-text-label">Диалоговое окно с успешно выполненными операциями</div>
     <Controls.operationsPopup:ReportDialog title="Заголовок"
                                            operationsCount="{{4}}"
                                            operationsSuccess="{{4}}">
     </Controls.operationsPopup:ReportDialog>
  </div>
  <div class="controlsDemo__wrapper__horizontal controlsDemo__cell">
     <div class="controls-text-label">Диалоговое окно с ошибкой</div>
     <Controls.operationsPopup:ReportDialog title="Заголовок"
                                            operationsCount="{{4}}"
                                            operationsSuccess="{{3}}">
     </Controls.operationsPopup:ReportDialog>
  </div>
</div>