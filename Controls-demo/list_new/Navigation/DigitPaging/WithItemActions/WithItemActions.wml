<div class="controlsDemo__wrapper">
    <Controls.list:View
            keyProperty="key"
            source="{{_viewSource}}"
            itemActionsPosition="outside"
            itemActions="{{_itemActions}}">
        <ws:navigation source="page" view="pages">
            <ws:sourceConfig pageSize="{{10}}" page="{{0}}" hasMore="{{false}}"/>
            <ws:viewConfig totalInfo="extended"/>
        </ws:navigation>
    </Controls.list:View>
</div>
