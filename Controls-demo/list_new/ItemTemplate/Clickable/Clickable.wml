<div class="controlsDemo__wrapper">
    <Controls.list:View keyProperty="key" source="{{_viewSource}}">
        <ws:itemTemplate>
            <ws:partial template="Controls/list:ItemTemplate"
                        displayProperty="value"
                        cursor="{{itemTemplate.item.contents.get('cursor')}}"
                        highlightOnHover="{{itemTemplate.item.contents.get('hovered')}}" />
        </ws:itemTemplate>
    </Controls.list:View>
</div>
